{
  "name": "astro-react-starter",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Micha≈Ç Strumpf",
  "private": true,
  "type": "module",
  "scripts": {
    "start:dev": "astro dev",
    "start:prod": "astro preview",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "ts:check": "tsc --noEmit",
    "lint:check": "eslint \"**/*.{ts,tsx,mjs,mts,astro}\"",
    "lint:fix": "eslint \"**/*.{ts,tsx,mjs,mts,astro}\" --fix",
    "format:check": "prettier \"**/*.{ts,tsx,mjs,mts,astro}\" --check --cache",
    "format:fix": "prettier \"**/*.{ts,tsx,mjs,mts,astro}\" --write --cache",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/react": "^4.2.7",
    "@fontsource/poppins": "^5.2.6",
    "@tailwindcss/vite": "^4.1.6",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "astro": "^5.7.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sharp": "^0.34.1",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.6",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin-ts": "^4.2.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "3.1.3",
    "eslint": "^9.26.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript-eslint": "^8.32.1",
    "vitest": "^3.1.3"
  }
}